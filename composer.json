{
  "name": "averinuveren2/lumen-pubsub",
  "description": "A Pub-Sub abstraction for Lumen",
  "license": "MIT",
  "authors": [
    {
      "name": "averinuveren2",
      "email": "averinuveren@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "illuminate/support": "^5.4",
    "superbalist/php-pubsub": "^1.0|^2.0",
    "superbalist/php-pubsub-redis": "^1.0|^2.0",
    "superbalist/php-pubsub-google-cloud": "^5.0",
    "illuminate/console": "^5.4",
    "superbalist/php-pubsub-http": "^1.0",
    "superbalist/php-pubsub-kafka": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Averinuveren\\LumenPubSub\\": "src/",
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5",
    "mockery/mockery": "^0.9.5",
    "kwn/php-rdkafka-stubs": "^1.3"
  }
}
